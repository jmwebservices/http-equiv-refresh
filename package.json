{
  "name": "http-equiv-refresh",
  "description": "Parse an HTML meta refresh value.",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Steven Vachon <contact@svachon.com> (https://www.svachon.com/)",
  "repository": "stevenvachon/http-equiv-refresh",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "chai": "^4.0.2",
    "del-cli": "^1.1.0",
    "merge-dirs": "^0.2.1",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^0.67.4"
  },
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "babel": "babel dist --out-dir dist",
    "BSI:build": "npm-run-all BSI:rollup babel BSI:merge BSI:clean",
    "BSI:clean": "del-cli dist/BSI",
    "BSI:merge": "merge-dirs dist/BSI ../BSI",
    "BSI:rollup": "rollup -c",
    "test": "mocha test.js --reporter=spec --check-leaks --bail"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "html",
    "http-equiv",
    "meta",
    "metadata",
    "parse",
    "refresh"
  ]
}
